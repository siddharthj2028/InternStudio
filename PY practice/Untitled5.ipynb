   "source": [
    "import math\n",
    "n = int(input())\n",
    "arr = list(map(int,input().split()))\n",
    "\n",
    "mean = sum(arr)/n\n",
    "\n",
    "sqd_diff_sum = 0\n",
    "for i in range(n):\n",
    "    diff = arr[i] - mean\n",
    "    sqd_diff_sum += diff **2\n",
    "    \n",
    "std_div = math.sqrt(sqd_diff_sum/n)\n",
    "print(f'std_div.2f')"
   ]
